<!DOCTYPE html>
<html>
<head>
    <title> Account - View All</title>
</head>
    <body>
        <h1><a href="/account/add">Add New Account</a> <br/></h1>

        <table border="1">
            <tr><th>First Name</th><th>Last Name</th><th>Email</th><th>Actions</th></tr>
            <% for(var i=0; account.length > i; i++ ) { %>
            <tr>
                <td><%= account[i].first_name %></td>
                <td><%= account[i].last_name %></td>
                <td><%= account[i].email %></td>
                <td>
                    <a href='/account/edit/?account_id=<%= account[i].account_id %>'>
                        Edit
                    </a>
                </td>
            </tr>
            <% } %>
        </table>
    </body>
</html>